{
  "name": "@trygg/docs-generator",
  "version": "0.0.1",
  "description": "Documentation generator for trygg - extracts TypeDoc and transforms to typed structures",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./extract": {
      "types": "./dist/typedoc-extractor.d.ts",
      "import": "./dist/typedoc-extractor.js"
    },
    "./transform": {
      "types": "./dist/transformer.d.ts",
      "import": "./dist/transformer.js"
    },
    "./generate": {
      "types": "./dist/generator.d.ts",
      "import": "./dist/generator.js"
    },
    "./validate": {
      "types": "./dist/validator.d.ts",
      "import": "./dist/validator.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "generate": "bun run src/cli.ts",
    "test": "echo 'No tests yet'"
  },
  "dependencies": {
    "@effect/cli": "^0.56.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-bun": "^0.87.1",
    "effect": "^3.19.15",
    "typedoc": "^0.27.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
